globalalloc(b1HitKill,1,$process)
globalalloc(pMahasti,4,$process)
globalalloc(pVizier,4,$process)

nHealthMax:
dd (int)1

b1HitKill:
db 1

// archer...
label(code)
label(return)

newmem:
  push ebx
  cmp byte ptr [b1HitKill],1
  jne code
  mov ebx,[nHealthMax]
  cmp [eax+000005F4],ebx
  jbe code
  mov [eax+000005F4],ebx
code:
  pop ebx
@@:
  mov ecx,[eax+000005F4]
  jmp return

Archer:
  jmp newmem
  nop
return:

// guard...
label(code2)
label(return2)

newmem2:
