code := "81 FA 01 00 00 00 0F 84 B9 00 00 00 81 FA A3 02 00 00 0F 84 AD 00 00 00 81 FA 84 03 00 00 "
. "0F 84 A1 00 00 00 81 FA 65 04 00 00 0F 84 95 00 00 00 81 FA 46 05 00 00 0F 84 89 00 00 00 " 
. "81 FA 27 06 00 00 0F 84 7D 00 00 00 81 FA 08 07 00 00 0F 84 71 00 00 00 81 FA CA 08 00 00 " 
. "0F 84 65 00 00 00 81 FA 8C 0A 00 00 0F 84 59 00 00 00 81 FA B8 0B 00 00 0F 84 4D 00 00 00 " 
. "81 FA 4E 0C 00 00 0F 84 41 00 00 00 81 FA 10 0E 00 00 0F 84 35 00 00 00 81 FA A6 0E 00 00 " 
. "0F 84 29 00 00 00 81 FA 94 11 00 00 0F 84 1D 00 00 00 81 FA 82 14 00 00 0F 84 11 00 00 00 " 
. "81 FA 70 17 00 00 0F 84 05 00 00 00 E9 05 00 00 00 BA 00 00 00 00 29 D0 89 4E 0C 89 07"

buy_code := "89 4E 0C C7 07 FF FF 00 00 BF FF FF 00 00"

items_code := "8B 01 89 02 48 C7 01 E7 03 00 00"

h_game := Get_process_handle(g_globals.target_name)
sleep, 2000

both  := new CodeCave(h_game, 0x101345DF, memlib_String2ByteArray(trim(code)), 1)
buy   := new CodeCave(h_game, 0x101344F1, memlib_String2ByteArray(trim(buy_code)), 1)
items := new CodeCave(h_game, 0x101348EC, memlib_String2ByteArray(trim(items_code)), 1)

if not g_globals.cheats
	suspend, on
return

c::
	soundplay, % both.switch()
return

~m::
	items._enable()
	soundplay, %A_windir%\media\Windows Error.wav
	keywait, m
return

~m UP::
	items._disable()
	soundplay, %A_windir%\media\Windows Ding.wav
return

~g::
	buy._enable()
	soundplay, %A_windir%\media\Windows Error.wav
	keywait, g
return

~g UP::
	buy._disable()
	soundplay, %A_windir%\media\Windows Ding.wav
return

