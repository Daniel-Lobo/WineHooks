dllcall(pex "InitFileHooks", str, path, astr, path, ptr, g_fhks.files.pc, ptr, 0, uint, flags)
return
for k, v in g_fhks.filesList
{
logerr(k " " v)
dllcall(pex "FileHooksInsertFile", wstr, k, wstr, v)
logerr(errorlevel)
}
}
