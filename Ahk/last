DllCall("AllocConsole")
else DllCall("AttachConsole", uint, (ATTACH_PARENT_PROCESS := -1))
hnd := DllCall("GetStdHandle", "int", -11, ptr)
}
return dllcall("WriteConsole", "ptr", hnd , "ptr", &msg, "int", strlen(msg), uint, 0, ptr, 0)
}
printl(msg = "") {
